{
  "name": "graph-fetch",
  "version": "1.0.0",
  "description": "MCP server for graph-based memory tools using Dgraph",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "inspector": "npx @modelcontextprotocol/inspector",
    "test:mcp": "echo '{\"jsonrpc\": \"2.0\", \"id\": 1, \"method\": \"tools/list\"}' | node dist/index.js"
  },
  "keywords": [
    "mcp",
    "dgraph",
    "ai",
    "memory",
    "graph-database"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/openai": "^2.0.1",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vercel/mcp-adapter": "^1.0.0",
    "ai": "^5.0.76",
    "cors": "^2.8.5",
    "dgraph-js": "^24.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "express": "^5.1.0",
    "neo4j-driver": "^5.26.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2"
  }
}
