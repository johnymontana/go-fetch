# Dgraph Configuration
DGRAPH_CONNECTION_STRING=dgraph://localhost:9080
DGRAPH_TIMEOUT=30

# Flask Server Configuration  
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_DEBUG=false

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Algorithm Configuration
DEFAULT_ALGORITHM_TIMEOUT=300
MAX_GRAPH_SIZE=100000
ENABLE_CACHING=true

# Scheduler Configuration
ENABLE_SCHEDULER=true
SCHEDULER_TIMEZONE=UTC

# Community Detection Algorithms
ENABLE_LOUVAIN=true
ENABLE_LABEL_PROPAGATION=true  
ENABLE_LEIDEN=true

# Centrality Algorithms
ENABLE_PAGERANK=true
ENABLE_BETWEENNESS=true
ENABLE_CLOSENESS=true
ENABLE_EIGENVECTOR=true

# Performance Settings
BATCH_SIZE=1000
PARALLEL_PROCESSING=true
MAX_WORKERS=4